{
  "manifest_version":3,
  "name":"NoClanker",
  "version":"1.0",
  "description":"Blocks many of the LLM websites, entire list of blocked LLM websites can be accessed at linkhere",
  "authors":"NSS",
  "permissions":[
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "web_accessible_resources":[
    {
      "resources":["blocked.html"],
      "matches":["<all_urls>"]
    }
  ] 
}
